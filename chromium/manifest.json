    "http://sme.sk/*", "http://*.sme.sk/*", "http://aktuality.sk/*", "http://*.aktuality.sk/*", "http://hnonline.sk/*", "http://*.hnonline.sk/*", "http://*.etrend.sk/*", "http://etrend.sk/*", "http://topky.sk/*", "http://*.topky.sk/*", "http://aktualne.sk/*", "http://*.aktualne.sk/*", "http://pravda.sk/*", "http://*.pravda.sk/*"
